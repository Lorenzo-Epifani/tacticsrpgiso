		class_ = char_data['class_']
		race = char_data['race']
	else:
		race=\"bangaa\"
		class_=\"gladiator\"
	portrait = \"res://assets/pg/races/{race}/{class}/portrait.png\".format({\"race\": race,\"class\":class_})


